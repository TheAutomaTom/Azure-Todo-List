{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "todo-list-data-factory"
		},
		"AdvWorksSqlServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdvWorksSqlServer'"
		},
		"AdvWorksBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AdvWorksBlob'"
		},
		"AdvWorksSqlServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "todolistsqlserver01.database.windows.net"
		},
		"AdvWorksSqlServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "todolistazuresqldb01"
		},
		"AdvWorksSqlServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admins-must-die"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "todo-list-data-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdvWorksSqlServer_password": {
						"value": "[parameters('AdvWorksSqlServer_password')]"
					},
					"AdvWorksBlob_connectionString": {
						"value": "[parameters('AdvWorksBlob_connectionString')]"
					},
					"AdvWorksSqlServer_properties_typeProperties_server": {
						"value": "[parameters('AdvWorksSqlServer_properties_typeProperties_server')]"
					},
					"AdvWorksSqlServer_properties_typeProperties_database": {
						"value": "[parameters('AdvWorksSqlServer_properties_typeProperties_database')]"
					},
					"AdvWorksSqlServer_properties_typeProperties_userName": {
						"value": "[parameters('AdvWorksSqlServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}